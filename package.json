      "prepare": "node ./.prepare/prepare.js && node ./.prepare/install/install.js && node ./.prepare/done.js",
      "dev": "nodemon --no-stdin",
      "use": "",
      "dev:extract": "cross-env EXTRACT=1 NODE_ENV=development nodemon --no-stdin",
      "dev:strip": "cross-env STRIP=1 NODE_ENV=development nodemon --no-stdin",
      "build": "reset && source $HOME/.nvm/nvm.sh; nvm use; cross-env NODE_ENV=production webpack  --config ./config/webpack.config.js",
      "build:debug": "reset && source $HOME/.nvm/nvm.sh; nvm use; cross-env DEBUG=1 NODE_ENV=production webpack  --config ./config/webpack.config.js",
      "build:fonts": "node ./.prepare/pre-build.js fonts",
      "build:images": "node ./.prepare/pre-build.js images",
      "build:assets": "node ./.prepare/pre-build.js fonts images",
      "fix": "eslint --fix src && stylelint 'src/scss/**/*.{css,scss}' --fix"
   },
